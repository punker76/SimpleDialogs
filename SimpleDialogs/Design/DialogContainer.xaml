<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:SimpleDialogs.Controls">
    <ControlTemplate x:Key="DialogContainerTemplate"
                     TargetType="controls:DialogContainer">
        
    </ControlTemplate>

    <Style TargetType="controls:DialogContainer">
        <Setter Property="Template"
                Value="{StaticResource DialogContainerTemplate}" />
    </Style>
</ResourceDictionary>